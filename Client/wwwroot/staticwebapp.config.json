{
  "routes": [
    {
      "route": "/api/todos",
      "methods": [ "GET", "POST", "PUT", "DELETE" ],
      "allowedRoles": [ "authorised" ]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/css/*", "/js/*", "/_framework/*", "/images/*", "/*.{png,jpg,gif,ico,svg,css,js,wasm,dll,dat,json}"]
  },
  "mimeTypes": {
    ".dll": "application/octet-stream",
    ".wasm": "application/wasm",
    ".dat": "application/octet-stream",
    ".json": "application/json"
  }
}